    "statusBarItem.errorBackground": "#ff6e60",
    "statusBarItem.errorForeground": "#ffffff",
    "statusBarItem.hoverBackground": "#ff8a8020",

    "tab.activeBackground": "#1a1a1a",
    "tab.activeForeground": "#ff8a80",
