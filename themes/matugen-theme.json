    "notificationToast.border": "#ff8a8060",
    "notifications.background": "#252525",
    "notifications.foreground": "#e8e8e8",
    "notifications.border": "#ff8a8060",
    "notificationLink.foreground": "#ffd180",
    "notificationsErrorIcon.foreground": "#ff6e60",
    "notificationsWarningIcon.foreground": "#ffd180",
    "notificationsInfoIcon.foreground": "#84a4bf",

    "peekView.border": "#ff8a80",
    "peekViewEditor.background": "#1a1a1a",
    "peekViewEditor.matchHighlightBackground": "#ffd18060",
    "peekViewEditorGutter.background": "#151515",
    "peekViewResult.background": "#252525",
    "peekViewResult.fileForeground": "#e8e8e8",
    "peekViewResult.lineForeground": "#a0a0a0",
    "peekViewResult.matchHighlightBackground": "#ffd18080",
    "peekViewResult.selectionBackground": "#ff8a80",
    "peekViewResult.selectionForeground": "#1a1a1a",
    "peekViewTitle.background": "#ff8a80",
    "peekViewTitleDescription.foreground": "#1a1a1a",
    "peekViewTitleLabel.foreground": "#1a1a1a",

    "merge.currentHeaderBackground": "#84a4bf50",
    "merge.currentContentBackground": "#84a4bf20",
    "merge.incomingHeaderBackground": "#ffd18050",
    "merge.incomingContentBackground": "#ffd18020",
    "merge.border": "#ff8a8060",
    "merge.commonHeaderBackground": "#ff8a8050",
    "merge.commonContentBackground": "#ff8a8020",

    "gitDecoration.addedResourceForeground": "#a8dadc",
    "gitDecoration.modifiedResourceForeground": "#84a4bf",
    "gitDecoration.deletedResourceForeground": "#ff6e60",
    "gitDecoration.untrackedResourceForeground": "#ffd180b3",
    "gitDecoration.ignoredResourceForeground": "#60606080",
    "gitDecoration.conflictingResourceForeground": "#ff6e60",
    "gitDecoration.submoduleResourceForeground": "#ff8a80",

    "terminal.background": "#1a1a1a",
    "terminal.foreground": "#e8e8e8",
    "terminal.selectionBackground": "#ff8a8060",
    "terminal.border": "#ff8a8040",
    "terminalCursor.foreground": "#ff8a80",
    "terminalCursor.background": "#1a1a1a",

    "terminal.ansiBlack": "#1a1a1a",
    "terminal.ansiRed": "#ff6e60",
    "terminal.ansiGreen": "#a8dadc",
    "terminal.ansiYellow": "#ffd180",
    "terminal.ansiBlue": "#84a4bf",
    "terminal.ansiMagenta": "#ff8a80",
    "terminal.ansiCyan": "#a8dadc",
    "terminal.ansiWhite": "#e8e8e8",

    "terminal.ansiBrightBlack": "#505050",
    "terminal.ansiBrightRed": "#ff9088",
    "terminal.ansiBrightGreen": "#c5f3f5",
    "terminal.ansiBrightYellow": "#ffe8b3",
    "terminal.ansiBrightBlue": "#a8c5df",
    "terminal.ansiBrightMagenta": "#ffb4ac",
    "terminal.ansiBrightCyan": "#c5f3f5",
    "terminal.ansiBrightWhite": "#ffffff",

    "breadcrumb.background": "#1a1a1a",
    "breadcrumb.foreground": "#a0a0a0",
    "breadcrumb.focusForeground": "#ff8a80",
    "breadcrumb.activeSelectionForeground": "#ffd180",
    "breadcrumbPicker.background": "#252525",

    "quickInput.background": "#252525",
    "quickInput.foreground": "#e8e8e8",
    "quickInputList.focusBackground": "#ff8a80",
    "quickInputList.focusForeground": "#1a1a1a",
    "quickInputList.focusIconForeground": "#ffd180",

    "settings.headerForeground": "#ff8a80",
    "settings.modifiedItemIndicator": "#ffd180",
    "settings.dropdownBackground": "#252525",
    "settings.dropdownForeground": "#e8e8e8",
    "settings.dropdownBorder": "#ff8a8050",
    "settings.checkboxBackground": "#252525",
    "settings.checkboxForeground": "#e8e8e8",
    "settings.checkboxBorder": "#ff8a8050",
    "settings.textInputBackground": "#252525",
    "settings.textInputForeground": "#e8e8e8",
    "settings.textInputBorder": "#ff8a8050",
    "settings.numberInputBackground": "#252525",
    "settings.numberInputForeground": "#e8e8e8",
    "settings.numberInputBorder": "#ff8a8050",

    "diffEditor.insertedTextBackground": "#a8dadc30",
    "diffEditor.insertedTextBorder": "#a8dadc60",
    "diffEditor.removedTextBackground": "#ff6e6030",
    "diffEditor.removedTextBorder": "#ff6e6060",
    "diffEditor.border": "#ff8a8050",
    "diffEditor.diagonalFill": "#30303080"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.readwrite",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#a8dadc"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ff6e60",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#ff8a80",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#ffd180"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#c0c0c0"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#84a4bf",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "storage.type.class"
      ],
      "settings": {
        "foreground": "#a8c5df",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#c5a68c"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float"
      ],
      "settings": {
        "foreground": "#ffb4ac"
      }
    },
    {
      "name": "Booleans & Constants",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#ff8a80",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#ffd180"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#ff8a80",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "text.html.basic entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#84a4bf",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS: Selectors",
      "scope": [
        "meta.selector",
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#a8dadc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS: Properties",
      "scope": [
        "support.type.property-name.css",
        "meta.property-name.css"
      ],
      "settings": {
        "foreground": "#84a4bf"
      }
    },
    {
      "name": "CSS: Values",
      "scope": [
        "support.constant.property-value.css",
        "constant.other.color.rgb-value.css"
      ],
      "settings": {
        "foreground": "#ffd180"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json"
      ],
      "settings": {
        "foreground": "#a8dadc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSON Values",
      "scope": [
        "meta.structure.dictionary.value.json"
      ],
      "settings": {
        "foreground": "#c5a68c"
      }
    },
    {
      "name": "Markdown: Headings",
      "scope": [
        "markup.heading",
        "heading.1.markdown",
        "heading.2.markdown",
        "heading.3.markdown",
        "heading.4.markdown",
        "heading.5.markdown",
        "heading.6.markdown"
      ],
      "settings": {
        "foreground": "#ff8a80",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown: Bold",
      "scope": [
        "markup.bold",
        "strong.markdown"
      ],
      "settings": {
        "foreground": "#ffd180",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown: Italic",
      "scope": [
        "markup.italic",
        "emphasis.markdown"
      ],
      "settings": {
        "foreground": "#84a4bf",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Links",
      "scope": [
        "markup.underline.link",
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#a8dadc",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown: Code",
      "scope": [
        "markup.inline.raw.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#c5a68c"
      }
    },
    {
      "name": "Markdown: Lists",
      "scope": [
        "beginning.punctuation.definition.list.markdown"
      ],
      "settings": {
        "foreground": "#ff8a80"
      }
    },
    {
      "name": "Types",
      "scope": [
        "support.type",
        "entity.name.type",
        "storage.type.primitive"
      ],
      "settings": {
        "foreground": "#a8c5df",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Decorators & Annotations",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "meta.annotation"
      ],
      "settings": {
        "foreground": "#ffd180",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#ff8a80",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "this, self, super",
      "scope": [
        "variable.language.this",
        "variable.language.self",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#ffb4ac",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#ffd180"
      }
    },
    {
      "name": "Template Strings",
      "scope": [
        "string.template",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#c5a68c"
      }
    },
    {
      "name": "Object Keys",
      "scope": [
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#a8dadc"
      }
    }
  ]
}
